#include <stdio.h>
#include <stdlib.h>

int maxof(const int a[], int elem_cnt)
{
    int max = foo[0];
    for(int i = 1; i < elem_cnt; i++){
      if(foo[i] > max) max = foo[i];
    }
    return max;
}

int main (void)
{
    int number;
    printf("사람 수: ");
    scanf("%d", &number);
    int *height = calloc(number, sizeof(int));
    printf("%d명의 키를 입력하세요.\n", number);
    for (int i = 0; i < number; i++){
        printf("height[%d]: ", i);
        scanf("%d", &height[i]);
    }
}